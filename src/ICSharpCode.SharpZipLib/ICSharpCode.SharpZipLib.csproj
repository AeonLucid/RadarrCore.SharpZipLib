<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>RadarrCore.SharpZipLib</PackageId>
    <SignAssembly>True</SignAssembly>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0-alpha3</Version>
    <RepositoryUrl>https://github.com/AeonLucid/RadarrCore.SharpZipLib</RepositoryUrl>
    <PackageReleaseNotes>This is a modified version.
Please use https://github.com/icsharpcode/SharpZipLib.</PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/AeonLucid/RadarrCore.SharpZipLib/blob/master/LICENSE.txt</PackageLicenseUrl>
